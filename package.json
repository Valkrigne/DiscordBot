  "description": "Simple Discord bot to manage my personal server embeds",
  "main": "src/index.ts",
  "scripts": {
    "dev": "NODE_ENV=development ts-node -r tsconfig-paths/register src/index.ts",
    "start": "NODE_ENV=production ts-node -r tsconfig-paths/register src/index.ts",
